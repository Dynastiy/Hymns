<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="stanlee.project.com.hymns">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/books_icon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name=".Hymns">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScreenSlidePagerActivity" />
        <activity
            android:name=".main_hymn_activity"
            android:label="@string/title_activity_main_hymn_activity"
            android:parentActivityName=".Hymns"
            android:theme="@style/MyMaterialTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="stanlee.project.com.hymns.Hymns" />
        </activity>
        <activity
            android:name=".SearchDialog"
            android:theme="@style/Theme.AppCompat.Light.Dialog" />
        <activity
            android:name=".SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName=".main_hymn_activity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="stanlee.project.com.hymns.Hymns" />
        </activity>
        <activity android:name=".AboutActivity"
            android:theme="@style/Theme.AppCompat.Light.Dialog"></activity>
    </application>

</manifest>